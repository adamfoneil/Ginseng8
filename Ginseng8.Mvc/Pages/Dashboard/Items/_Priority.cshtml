@model OpenWorkItemsResult

@if (Model.Priority.HasValue && Model.AssignedUserId.HasValue)
{
    string dueDate = (Model.MilestoneId != 0) ? $"by {Html.FormatValue(Model.MilestoneDate, "{0:ddd M/d}")}" : "without due date";
    <sup title="Priority @Model.Priority @dueDate" class="work-item-priority">@Model.Priority</sup>
}